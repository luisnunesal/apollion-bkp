{
  "name": "@captalys-platform/core",
  "version": "2.4.6",
  "description": "Component Library",
  "homepage": "https://apollion.captalysplatform.io/",
  "license": "MIT",
  "main": "lib/index.js",
  "module": "lib/index.esm.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/**/*"
  ],
  "repository": {
    "type": "git",
    "url": "git@gitlab.com:Captalys/interface/apollion.git"
  },
  "scripts": {
    "start": "tsc -w",
    "prepare": "yarn build",
    "prettier:write": "prettier src/**/*.ts{,x} --write",
    "test:watch": "jest --watch",
    "audit-dependencies": "audit-ci --config audit-ci.js",
    "coverage": "jest --coverage",
    "lint": "eslint src --quiet --ext ts,tsx --fix --ignore-path .gitignore",
    "check-types": "tsc --noEmit",
    "prettier": "prettier --check **/*.{ts,tsx} --ignore-path .gitignore --no-error-on-unmatched-pattern",
    "format": "yarn prettier --write",
    "validate": "./scripts/validate.sh",
    "validate:tests": "jest --coverage",
    "build": "node ./esbuild",
    "prebuild": "rimraf ./lib",
    "postbuild": "tsc --emitDeclarationOnly",
    "test": "yarn validate"
  },
  "peerDependencies": {
    "react": "*",
    "react-dom": "*"
  },
  "dependencies": {
    "@tippyjs/react": "4.2.0",
    "color": "3.1.3",
    "deepmerge": "4.2.2",
    "fast-deep-equal": "3.1.3",
    "formik": "2.2.9",
    "framer-motion": "4.1.17",
    "jest-styled-components": "7.0.2",
    "lodash.debounce": "4.0.8",
    "nanoid": "3.1.12",
    "object-hash": "2.2.0",
    "react-content-loader": "6.0.3",
    "react-router-dom": "5.2.0",
    "react-select": "4.3.1",
    "react-table": "7.7.0",
    "react-window": "1.8.6",
    "styled-components": "5.3.0",
    "styled-normalize": "8.0.6",
    "yup": "0.32.9"
  },
  "devDependencies": {
    "@captalys-platform/eslint-config-captalys-platform": "*",
    "@testing-library/dom": "7.29.0",
    "@testing-library/jest-dom": "4.1.2",
    "@testing-library/react": "9.3.0",
    "@testing-library/user-event": "12.6.0",
    "@types/color": "3.0.0",
    "@types/deepmerge": "2.2.0",
    "@types/jest": "24.0.21",
    "@types/lodash.debounce": "4.0.6",
    "@types/node": "15.12.5",
    "@types/object-hash": "2.1.0",
    "@types/react": "17.0.11",
    "@types/react-content-loader": "4.0.0",
    "@types/react-dom": "17.0.8",
    "@types/react-router-dom": "5.1.5",
    "@types/react-select": "4.0.16",
    "@types/react-table": "7.7.1",
    "@types/react-window": "1.8.3",
    "@types/styled-components": "5.1.4",
    "audit-ci": "3.1.1",
    "esbuild": "0.12.11",
    "jest": "27.0.6",
    "npm-run-all": "4.1.5",
    "prettier": "2.3.1",
    "ts-jest": "27.0.3",
    "typescript": "4.3.5"
  }
}
